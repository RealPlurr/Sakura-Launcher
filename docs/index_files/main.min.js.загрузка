!function(){const s=document,e=s.documentElement,i=s.body,t=(s.getElementById("lights-toggle"),window.sr=ScrollReveal());e.classList.remove("no-js"),e.classList.add("js"),window.addEventListener("load",function(){i.classList.add("is-loaded")}),i.classList.contains("has-animations")&&window.addEventListener("load",function(){t.reveal(".feature",{duration:600,distance:"20px",easing:"cubic-bezier(0.215, 0.61, 0.355, 1)",origin:"right",viewFactor:.2})}),i.classList.add("lights-off")}();